	keepBackup = false
[merge]
	tool = vsdiffmerge
[mergetool]
#	prompt = true
    keepBackup = false
[mergetool "vsdiffmerge"]
	cmd = \"C:\\Program Files\\Microsoft Visual Studio\\2022\\Enterprise\\Common7\\IDE\\CommonExtensions\\Microsoft\\TeamFoundation\\Team Explorer\\vsdiffmerge.exe\" \"$REMOTE\" \"$LOCAL\" \"$BASE\" \"$MERGED\" //m
	keepBackup = false
	trustExitCode = true
[push]
	default = upstream
#	autoSetupRemote = true
[fetch]
    prune = true
[pull]
    rebase = true
[core]
	editor = nvim
[alias]
    a   = add
    aa  = add .
    ap  = add -p
    b   = branch
    bd  = branch -d
    bD  = branch -D
    bdr = !pwsh -command Remove-GitRemoteBranch
    cl = !pwsh -command Git-CloseAndClean
    ch  = checkout
    chb = checkout -b
    cfg = config --global -e
    co  = commit
    com = commit -m
    coa = commit --amend
    con = commit --amend --no-edit
    d   = diff
    ds  = diff --staged
    f   = fetch
    l   = log
    g   = log --graph --oneline --branches
    ga  = log --graph --oneline --all
    mt  = mergetool
    ps  = push
    psf = push -f
    pl  = pull
    # ps  = !pwsh -command Invoke-GitPushDev
    # psf = !pwsh -command Invoke-GitPushDev -f
    rep = remote prune origin
    rs  = reset
    rsh = reset --hard
    rswip = reset head~
    rb  = rebase
    rbi = rebase -i
    rbc = rebase --continue
    rba = rebase --abort
    s   = status
